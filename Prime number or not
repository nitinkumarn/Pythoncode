n = int(input('Enter a number : '))
if n<=1:
    print('The {} value is not a prime number'.format(n))
else:
    is_prime = True
    if n >= 1:
        for i in range(2,n):
            if n%i ==0:
             is_prime = False
             break
    if(is_prime==True):
        print('The {} value is a prime number'.format(n))
    else:
        print('The {} value is not a prime number'.format(n))
