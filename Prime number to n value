n = int(input('Enter n value: '))
n1=2
count = 0
while n1<= n:
    is_prime = True
    for i in range(2,n1//2+1):
        if n1%i ==0:
            is_prime= False
            break
    if is_prime==True:
        count = count + 1
        print(n1)
        
    n1 = n1+1
print('Thenumber of prime numbers are : ',count)
